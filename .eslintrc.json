{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    // "plugins": [
    //     "react"
    // ],
    "rules": {
        "comma-dangle": ["error", "always-multiline"],
        "no-trailing-spaces": "error",
        "indent": ["error", 2],
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "error",
        "no-unreachable": "warn",
        "no-unused-vars": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "quotes": ["error", "single"]
    }
}